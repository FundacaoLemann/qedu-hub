{% set enrollmentsAggregation = censusData.getEnrollmentsAggregation() %}

<div class="group-educacenso">
  <h3>Matrículas</h3>
  <table class="table table-striped">
    <tbody>
    <tr>
      <th>Creche</th>
      <td>{{ enrollmentsAggregation.getNurseryEnrollments()|optionalNumberTranslationFilter }}</td>
    </tr>
    <tr>
      <th>Pré escola</th>
      <td>{{ enrollmentsAggregation.getPreSchoolEnrollments()|optionalNumberTranslationFilter }}</td>
    </tr>
    <tr>
      <th>Anos iniciais (1ª a 4ª série ou 1º ao 5º ano)</th>
      <td>{{ enrollmentsAggregation.getElementaryEnrollments()|optionalNumberTranslationFilter }}</td>
    </tr>
    <tr>
      <th>Anos finais (5ª a 8ª série ou 6º ao 9º ano)</th>
      <td>{{ enrollmentsAggregation.getMiddleEnrollments()|optionalNumberTranslationFilter }}</td>
    </tr>
    <tr>
      <th>Ensino Médio</th>
      <td>{{ enrollmentsAggregation.getHighSchoolEnrollments()|optionalNumberTranslationFilter }}</td>
    </tr>
    <tr>
      <th>Educação de Jovens e Adultos</th>
      <td>{{ enrollmentsAggregation.getEjaEnrollments()|optionalNumberTranslationFilter }}</td>
    </tr>
    <tr>
      <th>Educação Especial</th>
      <td>{{ enrollmentsAggregation.getSpecialEducationEnrollments()|optionalNumberTranslationFilter }}</td>
    </tr>
    </tbody>
  </table>
</div>